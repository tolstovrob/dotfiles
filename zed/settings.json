{
  "ui_font_family": "JetBrainsMono Nerd Font",
  "ui_font_size": 14,
  "ui_font_features": {
    "calt": true
  },
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_size": 16,
  "buffer_font_features": {
    "calt": true,
    "cv01": 7
  },
  "vim_mode": true,
  "project_panel": {
    "dock": "right"
  },
  "soft_wrap": "bounded",
  "preferred_line_length": 120,
  "git_diff": true,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Ayu Dark"
  }
}
